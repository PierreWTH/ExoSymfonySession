{% extends 'base.html.twig' %}

{% block title %} Ajouter une session {% endblock %}

{% block body %}

<div class="add-form-shadow">

    <h1 class="titre-page"> Ajouter une categorie </h1>

    <div class="add-form">
    {{form_start(formAddCategorie)}}
    {{form_row(formAddCategorie.nomCategorie)}}
    <div class="form-add-submit">
    {{form_row(formAddCategorie.ajouter)}}
    </div>
    {{form_end(formAddCategorie)}}
    </div>
</div>

{% endblock %}