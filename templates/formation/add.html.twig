{% extends 'base.html.twig' %}

{% block title %} Ajouter une formation {% endblock %}

{% block body %}

<div class="add-form-shadow">

    <h1 class="titre-page"> Ajouter une formation </h1>

    <div class="add-form">
    {{form_start(formAddFormation)}}
    {{form_row(formAddFormation.nomFormation)}}
    <div class="form-add-submit">
    {{form_row(formAddFormation.Ajouter)}}
    </div>
    {{form_end(formAddFormation)}}
    </div>
</div>

{% endblock %}