{% extends 'base.html.twig' %}

{% block title %} Liste des stagiaires {% endblock %}

{% block body %}

<h1> Informations de {{stagiaire.prenom}} {{stagiaire.nom}} </h1>

<p> Prenom  : {{stagiaire.prenom}} </p>
<p> Nom : {{stagiaire.nom}} </p>
<p> Date de naissance : {{stagiaire.dateNaissance |date('d/m/Y')}} </p>
<p> Telephone : {{stagiaire.telephone}} </p>
<p> Adresse : {{stagiaire.adresse}} </p>


{% endblock %}
